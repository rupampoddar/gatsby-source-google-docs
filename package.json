{
  "name": "google-docs-to-markdown",
  "version": "1.0.0",
  "description": "Generates markdown output from Google Docs json input",
  "main": "dist/gdoc-to-md.js",
  "module": "dist/gdoc-to-md.mjs",
  "typings": "dist/gdoc-to-md.d.ts",
  "dependencies": {
    "json2md": "^1.12.0",
    "lodash-es": "^4.17.21",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@tsconfig/node16": "^1.0.3",
    "@types/json2md": "^1.5.1",
    "@types/lodash-es": "^4.17.6",
    "@types/yamljs": "^0.2.31",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "^0.14.53",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "husky": "^7.0.0",
    "jest": "^28.0.3",
    "lint-staged": "^12.4.1",
    "prettier": "2.6.2",
    "rollup": "^2.77.2",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "semantic-release": "^19.0.2"
  },
  "scripts": {
    "cz": "git-cz",
    "lint": "eslint *.js utils examples/**/src",
    "test": "jest --coverage",
    "prepublishOnly": "npm lint && npm test",
    "semantic-release": "semantic-release",
    "prepare": "husky install",
    "prebuild": "rm -rf build",
    "build": "rollup -c",
    "prepack": "npm build"
  }
